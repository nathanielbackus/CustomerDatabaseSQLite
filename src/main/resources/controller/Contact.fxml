<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="227.0" prefWidth="353.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContactController">
    <children>
        <Label fx:id="contactLabel" layoutX="14.0" layoutY="14.0" style="-fx-font-size: 30;" text="Add Contact" />
        <Label layoutX="20.0" layoutY="70.0" text="ID:" />
        <Label layoutX="20.0" layoutY="110.0" text="Name:" />
        <Label layoutX="20.0" layoutY="150.0" text="Email:" />
        <TextField fx:id="ContactIDTextField" disable="true" editable="false" layoutX="150.0" layoutY="66.0" prefHeight="26.0" prefWidth="183.0" />
        <TextField fx:id="ContactNameTextField" layoutX="150.0" layoutY="106.0" prefHeight="26.0" prefWidth="183.0" />
        <TextField fx:id="ContactEmailTextField" layoutX="150.0" layoutY="146.0" prefHeight="26.0" prefWidth="183.0" />
        <Button fx:id="OnActionSaveContact" layoutX="199.0" layoutY="185.0" mnemonicParsing="false" onAction="#OnActionSaveContact" prefHeight="26.0" prefWidth="56.0" text="Save" />
        <Button fx:id="OnActionUsersContacts" layoutX="277.0" layoutY="185.0" mnemonicParsing="false" onAction="#OnActionUsersContacts" text="Cancel" />
    </children>
</AnchorPane>
